{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:react/recommended"],
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "modules": true,
          "allowImportExportEverywhere": true
      }
    },
  "rules": {
     "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}]
  }
}
